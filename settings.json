{
  "security.workspace.trust.untrustedFiles": "open",
  "editor.formatOnSave": true,
  "liveServer.settings.fullReload": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.wordWrap": "on",
  "kite.showWelcomeNotificationOnStartup": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/dist/css"
    }
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.sortImports": true
  },
  "eslint.validate": ["javascript"],
  "prettier.singleQuote": true,
  "editor.lineNumbers": "relative",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "bracketPairColorizer.depreciation-notice": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "indentRainbow.colors": [
    "rgba(255,255,64,0.07)",
    "rgba(127,255,127,0.07)",
    "rgba(255,127,255,0.07)",
    "rgba(79,236,236,0.07)"
  ],
  "indentRainbow.indicatorStyle": "light",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "indentRainbow.lightIndicatorStyleLineWidth": 2,
  "workbench.editorAssociations": {
    "*.eps": "default"
  },
  "eps-preview.path.pdf2svg": "pdf2svg",
  "editor.fontSize": 16,
  "editor.inlineSuggest.enabled": true,
  "workbench.colorTheme": "Eva Dark",
  "explorer.confirmDelete": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "better-comments.tags": [
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "?",
      "color": "#3498DB",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "//",
      "color": "#474747",
      "strikethrough": true,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#97EAD2",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    }
  ],
  "workbench.iconTheme": "vscode-icons",
  "hediet.vscode-drawio.resizeImages": null,
  "errorLens.delay": 2000,
  "cSpell.ignoreWords": [
    "createApi",
    "fetchBaseQuery",
    "inauthor",
    "renui",
    "signup"
  ],
  "cSpell.words": ["@rneui"],
  "cSpell.logLevel": "Information",
  "cSpell.allowCompoundWords": true,
  "git.enableSmartCommit": true,
  "tabnine.experimentalAutoImports": true,
  "workbench.editor.revealIfOpen": true,

  "vim.smartRelativeLine": true,
  "vim.ignorecase": true,
  "vim.leader": "<space>",

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-d>"],
      "after": ["<C-d>", "z", "z"]
    },
    {
      "before": ["<C-u>"],
      "after": ["<C-u>", "z", "z"]
    }
  ],

  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],

  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "a"],
      "commands": ["vscode-harpoon.addEditor"]
    },
    {
      "before": ["<leader>", "e"],
      "commands": ["vscode-harpoon.editEditors"]
    },
    {
      "before": ["<leader>", "p", "e"],
      "commands": ["vscode-harpoon.editorQuickPick"]
    },
    {
      "before": ["<leader>", "1"],
      "commands": ["vscode-harpoon.gotoEditor1"]
    },
    {
      "before": ["<leader>", "2"],
      "commands": ["vscode-harpoon.gotoEditor2"]
    },
    {
      "before": ["<leader>", "3"],
      "commands": ["vscode-harpoon.gotoEditor3"]
    },
    {
      "before": ["<leader>", "4"],
      "commands": ["vscode-harpoon.gotoEditor4"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "x"],
      "commands": ["workbench.action.moveEditorToNextGroup"]
    },
    {
      "before": ["<leader>", "z"],
      "commands": ["workbench.action.moveEditorToPreviousGroup"]
    },
    {
      "before": ["<leader>", "g", "d"],
      "commands": ["editor.action.peekDefinition"]
    },
    {
      "before": ["<leader>", "c", "a"],
      "commands": ["editor.action.quickFix"]
    }
  ],

  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    }
  ],

  "[python]": {
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  "workbench.startupEditor": "none",
  "window.zoomLevel": 1,
  "vsicons.dontShowNewVersionMessage": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "python": false,
    "rust": false
  },
 "workbench.colorCustomizations": {
    "editorInlayHint.foreground": "#adadadf0",
    "editorInlayHint.background": "#3d3d3d48",
    "editorCursor.foreground": "#786f86"
  },
  "cSpell.languageSettings": [],
  "cSpell.customDictionaries": {},
  "highlight-matching-tag.styles": "border: 1px solid rgba(255, 255, 255, 0.3);"
}
